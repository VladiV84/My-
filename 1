import sqlite3

con = sqlite3.connect('db.sqlite')
cur = con.cursor()

# Напишите SQL запрос в строке.
cur.execute('''
SELECT * FROM sqlite_master;
''')

table = con.cursor()  # Получите имя таблицы через атрибут курсора.

# Напишите SQL запрос в строке.
cur.execute('''
SELECT name, text
FROM sqlite_master;

''')

for result in cur:
    print(result)


con.commit()
con.close()
